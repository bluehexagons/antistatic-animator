{
  "name": "antistatic-animator",
  "version": "0.1.0",
  "description": "",
  "main": "dist/electron.js",
  "type": "commonjs",
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "esbuild ./src/electron.ts --bundle --platform=node --format=cjs --target=node20 --outfile=dist/electron.js --external:electron --external:electron-devtools-installer",
    "build:renderer": "vite build",
    "start": "electron .",
    "watch": "npm run build:main && npm run build:renderer -- --watch",
    "dist": "npx electron-packager ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluehexagons/antistatic-animator.git"
  },
  "author": "bluehexagons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bluehexagons/antistatic-animator/issues"
  },
  "homepage": "https://github.com/bluehexagons/antistatic-animator#readme",
  "devDependencies": {
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "esbuild": "^0.27.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^16.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "jsonc-parser": "^3.3.1"
  }
}
